<!DOCTYPE html>
<html>
<head>
  <title>Datadia</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if signed_in? %>
    <%= render "layouts/header" %>
  <% end %>
  <%= render "layouts/alerts" %>
  <div class="wrapper">
    <div class="container">
      <%= yield %>
    </div>
  </div>

</body>
</html>
